  },
  "license": "ISC",
  "author": "@andykenward",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --cache-location '.cache/eslint/' .",
    "lint:ls": "DEBUG=eslint:eslint pnpm run lint",
    "prepare": "husky",
    "prettier": "prettier . --write --cache --cache-location '.cache/prettier/'",
    "pretest": "pnpm --filter www build",
    "test": "playwright test",
    "test:ui": "pnpm run test --ui",
    "test:ui:container": "xvfb-run pnpm run test:ui --ui-host=0.0.0.0 --ui-port=8080"
  },
  "dependencies": {
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.5.1",
    "@playwright/test": "1.53.2",
    "@types/node": "22.17.0",
    "@typescript-eslint/parser": "8.37.0",
    "eslint": "9.31.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.11",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-packagejson": "^2.5.18",
    "prettier-plugin-tailwindcss": "0.6.14"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": "^22.15.0",
    "pnpm": "^10.12.4"
  }
