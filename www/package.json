{
  "name": "www",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev --port 4321",
    "preview": "astro preview --host --port 4321",
    "start": "astro dev --port 4321",
    "svgo:icons": "svgo --config svgo-icons.config.cjs -r -f ./src/assets/icons -o ./src/assets/icons",
    "svgo:svgs": "svgo --config svgo.config.cjs -r -f ./src/assets/svgs -o ./src/assets/svgs",
    "tsc:check": "tsc --noEmit --checkJs"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/sitemap": "3.6.0",
    "@fontsource/atkinson-hyperlegible": "5.2.8",
    "@fontsource/righteous": "5.2.7",
    "@tailwindcss/vite": "4.1.17",
    "astro": "5.16.3",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "svgo": "4.0.0"
  }
}
