{
  "name": "www",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "preview": "astro preview --host",
    "start": "astro dev",
    "svgo:icons": "svgo --config svgo-icons.config.cjs -r -f ./src/assets/icons -o ./src/assets/icons",
    "svgo:svgs": "svgo --config svgo.config.cjs -r -f ./src/assets/svgs -o ./src/assets/svgs",
    "tsc:check": "tsc --noEmit --checkJs"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/sitemap": "3.5.1",
    "@fontsource/atkinson-hyperlegible": "5.2.6",
    "@fontsource/righteous": "5.2.6",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "5.13.3",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "svgo": "4.0.0"
  }
}
